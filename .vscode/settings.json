{
    "i18n-ally.keystyle": "nested",
    "typescript.tsdk": "node_modules/typescript/lib",
    "cSpell.words": [
        "cici",
        "cicipanel",
        "dont",
        "firstname",
        "lastname",
        "verif",
        "writables"
    ],
    "i18n-ally.localesPaths": ["src/lib/i18n/locales"],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "*-lock.json": true,
        "*.lock": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.vscode": true,
        "**/node_modules": true,
        ".svelte-kit": true,
        "drizzle": true
    },
    "explorerExclude.backup": {},
    "i18n-ally.extract.ignoredByFiles": {
        "src/routes/app/moderation/users/create/+page.svelte": [
            " {\n                e.preventDefault();\n                passwordVisible = !passwordVisible;\n              }}\n              type=\"button\"\n              class=\"flex items-center justify-center\"\n              title={$_('auth.show-hide-password')}\n            >\n              "
        ]
    },
    "i18n-ally.extract.ignored": [
        "\n\n      {#if\n        (\n          data.userPerms.canSetGroups\n          || data.userPerms.canSetRoles\n          || data.userPerms.canSetPermissions\n        ) ",
        "&",
        "&"
    ]
}
