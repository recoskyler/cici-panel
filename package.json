{
  "name": "cici-panel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ext .js,.svelte,.ts src",
    "fix-lint": "eslint --fix --ext .js,.svelte,.ts src",
    "generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
    "studio": "drizzle-kit studio",
    "start": "node build",
    "reset-db": "docker compose down --volumes && rm -rf ./drizzle && docker compose up -d && npm run generate"
  },
  "devDependencies": {
    "@playwright/test": "^1.38.0",
    "@skeletonlabs/skeleton": "^2.1.0",
    "@skeletonlabs/tw-plugin": "^0.2.0",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "^1.25.0",
    "@tailwindcss/forms": "^0.5.6",
    "@types/node": "^20.6.2",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "autoprefixer": "^10.4.15",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.49.0",
    "eslint-plugin-svelte": "^2.33.2",
    "postcss": "^8.4.30",
    "postcss-load-config": "^4.0.1",
    "sass": "^1.67.0",
    "svelte": "^4.2.0",
    "svelte-adapter-bun": "^0.5.0",
    "svelte-check": "^3.5.2",
    "sveltekit-rate-limiter": "^0.4.1",
    "sveltekit-superforms": "^1.7.0",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-tailwind-purgecss": "^0.1.3",
    "zod": "^3.22.2"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "^1.5.3",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@lucia-auth/adapter-postgresql": "^2.0.1",
    "@types/validator": "^13.11.1",
